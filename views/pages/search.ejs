<script language="javascript">
  document.title = "'<%= search %>'에 대한 검색 결과 :: pracler" ;
    $('#side-tab a').click(function (e) {
        e.preventDefault();
        
        $('a').removeClass('active');
        $(this).addClass('active');
    })
</script>

<script>
  $(function(){ 
    var search = "<%= search %>vv";
    var urlmusic = "/viewcall/search/music/"+search;
    var urlalbum = "/viewcall/search/album/"+search;
    var urlartist = "/viewcall/search/artist/"+search;
    var urluser = "/viewcall/search/user/"+search;

    $.ajax({
        url: urlmusic,
        type: 'GET',
        dataType: 'html',
        success: function(result) {
            $('#res_music').html(result);
        }
    });	
    $.ajax({
        url: urlalbum,
        type: 'GET',
        dataType: 'html',
        success: function(result) {
            $('#res_album').html(result);
        }
    });	
    $.ajax({
        url: urlartist,
        type: 'GET',
        dataType: 'html',
        success: function(result) {
            $('#res_artist').html(result);
        }
    });	
    $.ajax({
        url: urluser,
        type: 'GET',
        dataType: 'html',
        success: function(result) {
            $('#res_user').html(result);
        }
    });	
  })

</script>
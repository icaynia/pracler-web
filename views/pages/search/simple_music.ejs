<h4> 음악 </h4>
<table class="table table-striped table-hover ">
<thead>
    <tr>
        <th>#</th>
        <th>아티스트</th>
        <th>앨범</th>
        <th>제목</th>
        <th>분류</th>
        <th>재생</th>
    </tr>
</thead>
<tbody>

    <% for(var i=0; i < data.length; i++) {%>
        <tr>
            <td></td>
            <td><a href="/song/<%=data[i].artist %>"><%=data[i].artist %></a></td>

            <% if (data[i].album != "_") { %>
                <td><a href="/song/<%=data[i].artist %>/<%=data[i].album %>"><%=data[i].album %></a></td>
            <% } else { %>
                <td></td>
            <% } %>
            <td><a href="/song/<%=data[i].artist %>/<%=data[i].album %>/<%=data[i].title %>"><%=data[i].title %></a></td>
            <td>-</td>
            <% if (data[i].youtube_code != "null") { %>
                <td class="youtube-play-bt" code="<%= data[i].youtube_code %>"><a href="#">재생(youtube)</a></td>
            <% } else if (data[i].soundcloud_code != "null" ){ %>
                <td class="soundcloud-play-bt" code="<%= data[i].soundcloud_code %>"><a href="#">재생(soundcloud)</a></td>
            <% } else { %>
                <td></td>
            <% } %>
        </tr>
    <% }; %>
            
</tbody>
</table> 

<ul class="pager">
    <li class="next"><a href="#">더 보기 &rarr;</a></li>
</ul>

<script language="javascript">

    $('.youtube-play-bt').click(function (e) {
        e.preventDefault();
        youtube_setContentPlayer_autoplay($(this).attr('code'));
    });

    $('.soundcloud-play-bt').click(function (e) {
        e.preventDefault();
        soundcloud_setContentPlayer_autoplay($(this).attr('code'));
    })
</script>